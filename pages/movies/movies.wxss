.nav {
  padding-top: 7px;
  height: 35px;
  width: 100%;
  display: flex;
  background-color: #ffffff;
  border-bottom: 1px solid gray;
  flex-direction: row;
  justify-content: flex-start;
  position: fixed;
  top: 0;
}


.nav ul {
  padding-top: 2px;
  flex: 1;
}

.nav ul li {

  padding-right: 2.65%;
  padding-left: 2.65%;
  font-size: 14px;
  font-weight: 700;

}

.one {
  border-bottom: 3px solid red;
}

